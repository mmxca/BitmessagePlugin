{
    "name": "mmxca/bitmessage-plugin",
    "description": "BitMessagePlugin plugin for CakePHP3",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "soundasleep/html2text": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "BitmessagePlugin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BitmessagePlugin\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
